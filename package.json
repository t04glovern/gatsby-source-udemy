{
  "name": "gatsby-source-udemy",
  "description": "Gatsby source plugin for building websites using Udemy as a data source",
  "version": "1.0.0",
  "author": "Nathan Glover <nathan@glovers.id.au>",
  "bugs": {
    "url": "https://github.com/t04glovern/gatsby-source-udemy/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "babel-preset-gatsby-package": "^0.2.0",
    "cross-env": "^5.1.4"
  },
  "homepage": "https://github.com/t04glovern/gatsby-source-udemy/#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "udemy"
  ],
  "license": "MIT",
  "main": "src/gatsby-node.js",
  "peerDependencies": {
    "gatsby": "^2.0.15"
  },
  "repository": "https://github.com/t04glovern/gatsby-source-udemy",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
